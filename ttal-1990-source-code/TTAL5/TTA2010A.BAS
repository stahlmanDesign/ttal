SUB SKULL1IFS

     IF S1 > X0 AND POINT(S1 - 3, K1 + 8) = BG THEN S1 = S1 - L1
     IF S1 < X0 AND POINT(S1 + 18, K1 + 8) = BG THEN S1 = S1 + L1
     IF K1 > Y0 AND POINT(S1 + 8, K1 - 4) = BG THEN K1 = K1 - L1
     IF K1 < Y0 AND POINT(S1 + 8, K1 + 18) = BG THEN K1 = K1 + L1

                    'DOES SKULL1 HURT MAN0

IF S1 < X0 + 10 AND S1 > X0 - 10 THEN IF K1 < Y0 + 10 AND K1 > Y0 - 10 THEN RANDOMIZE TIMER: IF INT(RND(1) * 5 + 1) < 3 THEN SKULL1HURTMAN0

IF S1 < 14 THEN S1 = S1: U5 = ABS(U5)
IF S1 + 14 > 314 THEN S1 = S1: U5 = ABS(U5)
IF K1 < 9 THEN K1 = K1: U5 = ABS(U5)
IF K1 + 14 > 189 THEN K1 = K1: U5 = ABS(U5)

END SUB

