SUB DEMON1IFS

     IF W1 > X0 AND POINT(W1 - 3, D1 + 8) = BG THEN W1 = W1 - T1
     IF W1 < X0 AND POINT(W1 + 18, D1 + 8) = BG THEN W1 = W1 + T1
     IF D1 > Y0 AND POINT(W1 + 8, D1 - 3) = BG THEN D1 = D1 - T1
     IF D1 < Y0 - 10 AND POINT(W1 + 8, D1 + 18) = BG THEN D1 = D1 + T1

IF W1 < X0 + 20 AND W1 > X0 - 20 THEN IF D1 < Y0 + 20 AND D1 > Y0 - 20 THEN RANDOMIZE TIMER: IF INT(RND(1) * 5 + 1) < 3 THEN DEMON1STAB

IF W1 < 14 THEN W1 = W1: U3 = ABS(U3)
IF W1 + 14 > 314 THEN W1 = W1: U3 = ABS(U3)
IF D1 < 9 THEN D1 = D1: U3 = ABS(U3)
IF D1 + 14 > 189 THEN D1 = D1: U3 = ABS(U3)

END SUB

SUB DEMON1MOVE

     IF Q3 = 1 THEN PUT (W1, D1), G1: Q3 = 2

     PUT (W1, D1), G1
     PUT (W1, D1), I1: FOR Q = 1 TO PT: NEXT: PUT (W1, D1), I1
     DEMON1IFS
    
     IF V3 > 0 THEN SOUND 160, 1: SOUND 39, 1: U3 = ABS(U3): V3 = 0: Q3 = 1: W1 = 0: D1 = 0: EXIT SUB

     PUT (W1, D1), G1: FOR Q = 1 TO PT: NEXT
     
END SUB

SUB DEMON1STAB
    
     D1 = D1 - (T1 + 5): PUT (W1, D1), K1: DEMON1STABIFS: PUT (W1, D1), K1: D1 = D1 + T1

END SUB

SUB DEMON1STABIFS

FOR Q = 1 TO PT + 75: NEXT

IF X0 + 14 > W1 AND X0 < W1 + 10 THEN IF Y0 > D1 + 14 AND Y0 < D1 + 23 THEN SOUND 70, 2: SOUND 180, 1: HP = HP - 15: HEALTHSCALE

END SUB

SUB DEMON2IFS

     IF W2 > X0 AND POINT(W2 - 3, D2 + 8) = BG THEN W2 = W2 - T2
     IF W2 < X0 AND POINT(W2 + 18, D2 + 8) = BG THEN W2 = W2 + T2
     IF D2 > Y0 AND POINT(W2 + 8, D2 - 3) = BG THEN D2 = D2 - T2
     IF D2 < Y0 - 10 AND POINT(W2 + 8, D2 + 18) = BG THEN D2 = D2 + T2

IF W2 < X0 + 20 AND W2 > X0 - 20 THEN IF D2 < Y0 + 20 AND D2 > Y0 - 20 THEN RANDOMIZE TIMER: IF INT(RND(1) * 5 + 1) < 3 THEN DEMON2STAB

